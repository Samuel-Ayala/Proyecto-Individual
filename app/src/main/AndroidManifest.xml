<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pe.pucp.dduu.proyecto_individual">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icono_app"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icono_app_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".ActivitiesProfesor.EditarTareaActivity"></activity>
        <activity android:name=".ActivitiesProfesor.ListaEditTareasActivity" />
        <activity android:name=".ActivitiesProfesor.QuejasActivity" />
        <activity android:name=".ActivitiesProfesor.ListaEstudiantesActivity" />
        <activity android:name=".ActivitiesProfesor.GestionNotasActivity" />
        <activity android:name=".ActivitiesProfesor.CrearTareaActivity" />
        <activity android:name=".ActivitiesApoderado.TareasActivity" />
        <activity android:name=".ActivitiesApoderado.NotasActivity" />
        <activity android:name=".ActivitiesApoderado.EnviarQuejaActivity" />
        <activity android:name=".ActivitiesApoderado.ApoderadoActivity" />
        <activity android:name=".ActivitiesProfesor.ProfesorActivity" />
        <activity android:name=".ActivitiesGeneral.LoginRegistroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>